#include <PRIZM.h>      
PRIZM prizm;

void setup() {
   prizm.PrizmBegin();
   //Pg165
   prizm.setMotorInvert(2,1);
}
void loop() { 
   //Drive forward 1m  Pg 164
   prizm.setMotorSpeeds(360,360);
   delay(1000);
   prizm.setMotorSpeeds(0,360);
   delay(2000);
   prizm.setMotorSpeeds(180,180);
   delay(1000);
   prizm.setMotorSpeeds(0,0);
   delay(1000);
   prizm.setMotorSpeeds(360,0);
   delay(1000);
   prizm.setMotorSpeeds(180,180);
   delay(1000);
   prizm.setMotorSpeeds(0,0);
   delay(100);
   prizm.PrizmEnd();
}
